<p id="notice"></p>
 <div class="container"> 
  <div class="row">    
  
   <div class="col-md-3">
<%= render 'users', user: @user %>
       

<%= render 'books/books', book: @book %>


   </div>
   
  <div class="col-md-8 offset-md-1">
<h2>User</h2>
<table class="table table-striped">

<thead>
 <tr>
  <th>image</th>
  <th>name</th>
 </tr>
</thead>

 <% @users.each do |user| %>

<tbody>
 <tr>
  <td><%= image_tag user.get_profile_image %></td>
